{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/christos-karalis/spring-angular2.git"
  },
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js --debug",
    "postinstall": "typings install",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.16",
    "es6-shim": "^0.35.0",
    "express": "4.13.4",
    "ng2-bootstrap": "1.0.15",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "serve-static": "1.10.2",
    "systemjs": "0.19.25",
    "typescript": "1.8.10",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "remap-istanbul": "^0.5.1",
    "lite-server": "^2.1.0",
    "systemjs": "^0.19.25",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  }
}
