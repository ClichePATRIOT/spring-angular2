{
  "name": "spring-angular2",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/christos-karalis/spring-angular2.git"
  },
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js --debug",
    "postinstall": "typings install",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",

    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23",

        "express": "4.13.4",
        "serve-static": "1.10.2",

    "angular2-in-memory-web-api": "0.0.20",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "remap-istanbul": "^0.5.1",
    "lite-server": "^2.1.0",
    "systemjs": "^0.19.25",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  }
